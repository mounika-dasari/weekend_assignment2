<!DOCTYPE HTML>
<HTML>
<HEAD>
<script>
let a1=[2,4,4,6,6,1,3,5,7,9,10,1,1];
let removeDuplicates=function(arr1){
	let arr=arr1.filter(function(x,i,arr1){
		let flag=true;
		arr1.splice(i,1);
		if(arr1.includes(x))
		flag=false;
		arr1.splice(i,0,x);
		return flag;
	});
	return arr;
}
console.log(removeDuplicates(a1));

console.log(function(){
	let arr=a1.filter(function(x,i,arr1){
		let flag=true;
		arr1.splice(i,1);
		if(arr1.includes(x))
		flag=false;
		arr1.splice(i,0,x);
		return flag;
	});
	return arr;
}());
</script>
